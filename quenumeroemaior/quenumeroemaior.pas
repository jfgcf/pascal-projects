Program quenumeroemaior ;
Uses crt;
var n1, n2: real;

 Begin
 write('Introduza o primeiro numero: ');
 read(n1);
 write('Introduza o segundo numero: ');
 read(n2);
 If (n1>n2) then
 writeln('O primeiro numero e maior que o segundo')
 else
 writeln('O segundo numero e maior que o primeiro'); 
 End.
