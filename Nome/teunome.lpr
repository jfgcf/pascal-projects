program teunome;

var nome, apelido: string;

begin
  Write('Introduz o teu nome: ');
  readln(nome);
  writeln;{new line}
  Write('Introduz o teu apelido: ');
  readln(apelido);
  writeln;{new line}
  writeln;{new line}
  writeln('O teu nome e: ',nome,' ',apelido);
  readln;


end.

